        app:srcCompat="@drawable/images__8_" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="25mm"
        android:layout_height="25mm"
        android:layout_marginStart="33mm"
        android:layout_marginTop="10mm"
        app:srcCompat="@drawable/images__7_" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="25mm"
        android:layout_height="25mm"
        android:layout_marginStart="3mm"
        android:layout_marginTop="100mm"
        app:srcCompat="@drawable/photo2jpg" />



    <Button
        android:id="@+id/back11"
        android:layout_width="10mm"
        android:layout_height="5mm"
        android:text="back"
        android:background="@color/colorRed"
        android:textAlignment="center"
        android:layout_marginStart="3mm"
        android:layout_marginTop="130mm"
         />


</androidx.coordinatorlayout.widget.CoordinatorLayout>